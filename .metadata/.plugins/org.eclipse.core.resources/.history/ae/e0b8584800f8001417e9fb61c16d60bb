package PakietyPodstawowe.Model.Interfejsy;

public interface IIO {
	
	public static void drukuj(String wiadomoœæDoWydrukowania) {
		System.out.print(wiadomoœæDoWydrukowania);
	}
	public static void drukuj(String wiadomoœæDoWydrukowania, boolean czyZnakNowejLinii) {
		czyZnakNowejLinii == true ? System.out.println(wiadomoœæDoWydrukowania) : drukuj(wiadomoœæDoWydrukowania);
	}

}
