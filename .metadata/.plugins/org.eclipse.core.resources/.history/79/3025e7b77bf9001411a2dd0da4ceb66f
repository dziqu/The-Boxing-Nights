package Enumy;

public enum ELokalizacjeINazwy {
	
	FOLDER_ZAPISU_STANÓW_GRY("zapisaneStanyGry"),
	FOLDER_USTAWIEN("ustawienia"),
	ZAPISANE_USTAWIENIA_WYŒWIETLANIA("ustawienia/ustawienia");
    
    private final String lokalizacja;
    
    private ELokalizacjeINazwy(String lokalizacja) {
        this.lokalizacja = lokalizacja;
    }
    
    public String zwróæLokalizacjê() {
        return lokalizacja;
    }

}
