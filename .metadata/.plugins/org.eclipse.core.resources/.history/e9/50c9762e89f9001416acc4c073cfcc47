package Inne;

import java.util.Comparator;

public class Porównania <T> {
	
//	public static Integer porównaj(String s1, String s2) { return cStr.compare(s1, s2); }
//	
//	public static Integer porównaj(Integer i1, Integer i2) { return cInt.compare(i1, i2); }
//	
//	public static Integer porównaj(Float f1, Float f2) { return cFloat.compare(f1, f2); }
//	
//	public static Integer porównaj(Double d1, Double d2) { return cDouble.compare(d1, d2); }
//	
//	public static Integer porównaj(Boolean b1, Boolean b2) { return cBool.compare(b1, b2); }
//	
//	public static Integer porównaj(Character c1, Character c2) { return cChar.compare(c1, c2); }
	
	public static <T> Integer porównaj(T t1, T t2) {
		IO.drukujL(t1.getClass());
		return 0;
	}
	
	private static Comparator <String> cStr = (s1, s2) -> s1.compareTo(s2);
	private static Comparator <Integer> cInt = (i1, i2) -> i1.compareTo(i2);
	private static Comparator <Float> cFloat = (f1, f2) -> f1.compareTo(f2);
	private static Comparator <Double> cDouble = (d1, d2) -> d1.compareTo(d2);
	private static Comparator <Boolean> cBool = (b1, b2) -> b1.compareTo(b2);
	private static Comparator <Character> cChar = (c1, c2) -> c1.compareTo(c2);
}
