package PakietyPodstawowe.Model.Enumy;

public enum ERenderery {
	
	OpenGL1("LWJGL-OPENGL1"), OpenGL2("LWJGL-OpenGL2"), OpenGL3("LWJGL-OpenGL3");
    
    private final String renderer;
    
    private ERenderery(String renderer) {
        this.renderer = renderer;
    }
    
    public String zwróæRenderer() {
        return renderer;
    }

}
