package Inne;

import com.jme3.input.KeyInput;
import com.jme3.input.controls.KeyTrigger;
import com.jme3.input.controls.Trigger;

public class SterowanieGraczem1 {
	
	private final Trigger TRIGGER_DO_PRZODU;
	
	public SterowanieGraczem1() {
		TRIGGER_DO_PRZODU = new KeyTrigger(KeyInput.KEY_A);
	}
	
	public Integer zwróæZnak(String wiadomoœæ) {
		Integer znak = null;
		if (Porównania.porównaj(wiadomoœæ, "q") == 0) znak = KeyInput.KEY_Q;
		
		return znak;
	}

}
