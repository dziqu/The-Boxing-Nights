package Model.Enumy;

/**
 *
 * @author Filip
 */
public enum EBuforSzablonowy {
    WYŁĄCZ(0), WŁĄCZ(8);
    
    private final Integer liczbaBitówMatrycy;
    
    private EBuforSzablonowy(Integer liczbaBitówMatrycy) {
        this.liczbaBitówMatrycy = liczbaBitówMatrycy;
    }
    
    public Integer zwróćLiczbęBitówMatrycy() {
        return liczbaBitówMatrycy;
    }
}
