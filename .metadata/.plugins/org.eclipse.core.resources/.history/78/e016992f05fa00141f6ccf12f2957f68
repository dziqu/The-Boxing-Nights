package Enumy;

public enum ENazwy {
	
	FOLDER_ZAPISU_STANÓW_GRY("zapisaneStanyGry\\"),
	FOLDER_USTAWIEÑ("ustawienia"),
	PLIK_USTAWIEÑ_WYSWIETLANIA(FOLDER_USTAWIEÑ.zwróæLokalizacjê() + "//ustawieniaWyswietlania.txt"),
	PLIK_USTAWIEÑ_GRY("theBoxingNightsUstawienia");
    
    private final String lokalizacja;
    
    private ENazwy(String lokalizacja) {
        this.lokalizacja = lokalizacja;
    }
    
    public String zwróæLokalizacjê() {
        return lokalizacja;
    }

}
