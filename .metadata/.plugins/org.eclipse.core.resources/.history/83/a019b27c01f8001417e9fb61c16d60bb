package PakietyPodstawowe.Model.Interfejsy;

import java.util.List;

public interface IIO {
	
	public static <T> void drukuj(T s) {
		System.out.print(s);
	}
	public static <T> void drukuj(List<T> lista) {
		lista.forEach(s -> drukuj(s));
	}
	public static void drukujL(T s) {
		System.out.println(s);
	}
	public static <T> void drukujL(List<T> lista) {
		lista.forEach(s -> drukujL(s));
	}

}
